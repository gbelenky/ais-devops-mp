{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "storageNamePrefix": {
            "type": "string",
            "maxLength": 11,
            "defaultValue": "storage",
            "metadata": {
            "description": "The prefix value for the storage account name."
            }
        },
        "location": {
            "value": "westeurope"
        },
        "environmentName": {
            "value": "env-name"
        },
        "projectName": {
            "value": "project-name"
        },
        "logicAppName": {
            "value": "la-name"
        },
        "appServicePlanName": {
            "value": "asp-name"
        },
        "storageName": {
            "value": "[concat(parameters('storageNamePrefix'), uniquestring(resourceGroup().id))]"
        },
        "kind": {
            "value": "StorageV2"
        },
        "skuName": {
            "value": "Standard_LRS"
        },
        "skuTier": {
            "value": "Standard"
          }
        }
    }
